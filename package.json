{
  "name": "tugboat-wp",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run build && npm run link && npm run build:watch",
    "lint": "npx eslint ./src",
    "build": "npx babel src --delete-dir-on-start --out-dir lib",
    "build:watch": "npx nodemon --exec npm run build",
    "clean": "rm -rf ./lib",
    "link": "npm link",
    "unlink": "npm unlink tugboat-wp",
    "setup": "npm run build && npm run link",
    "remove": "npm run unlink && npm run clean"
  },
  "author": "Aaron Mazade",
  "license": "ISC",
  "description": "The Tugboat WP CLI is a command-line interface (CLI) tool designed to simplify and streamline WordPress development workflows.",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^5.1.0",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-jest": "^26.6.3",
    "babel-preset-minify": "^0.5.1",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*",
      "coverage/*"
    ]
  },
  "bin": {
    "tugboat-wp": "./lib/commands/tugboat-wp/tugboat-wp.js"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
